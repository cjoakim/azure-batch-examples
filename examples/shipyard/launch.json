{
  "docker_image": "cjoakim/azure-batch-shipyard1:latest",
  "xpool_id": "cjoakimpool7",
  "storage_acct": "cjoakimstdstorage",
  "vm_size": "Standard_DS3_v2",
  "vm_count": 1,
  "job_basename": "cjoakimshipyard",
  "job_env_var_names": [
    "AZURE_STORAGE_ACCOUNT",
    "AZURE_STORAGE_KEY",
    "AZURE_SERVICEBUS_NAMESPACE",
    "AZURE_SERVICEBUS_KEY_NAME",
    "AZURE_SERVICEBUS_ACCESS_KEY",
    "AZURE_SERVICEBUS_QUEUE"
  ],
  "command": "docker run -d -e AZURE_STORAGE_ACCOUNT=$AZURE_STORAGE_ACCOUNT -e AZURE_STORAGE_KEY=$AZURE_STORAGE_KEY cjoakim/azure-batch-shipyard1:latest"
}
