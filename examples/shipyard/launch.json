{
  "docker_image": "cjoakim/azure-batch-shipyard1:latest",
  "pool_id": "cjoakimpool0803",
  "storage_acct": "cjoakimstdstorage",  
  "vm_size": "Standard_DS3_v2",
  "vm_size_prev": "Standard_A5", 
  "vm_count": 1,
  "job_basename": "pyjob",
  "job_env_var_names": [
    "AZURE_STORAGE_ACCOUNT",
    "AZURE_STORAGE_KEY"
  ],
  "task_parameters": [
    {
      "input_container": "batchin",
      "input_blob": "postal_codes_ct.csv",
      "output_container": "batchout",
      "output_blob": "result_ct.csv",
      "logging_container": "batchlog"
    },
    {
      "input_container": "batchin",
      "input_blob": "postal_codes_fl.csv",
      "output_container": "batchout",
      "output_blob": "result_fl.csv",
      "logging_container": "batchlog"
    },
    {
      "input_container": "batchin",
      "input_blob": "postal_codes_ga.csv",
      "output_container": "batchout",
      "output_blob": "result_ga.csv",
      "logging_container": "batchlog"
    }
  ],
  "command": "python /app/pandas1.py --function state_center"
}
